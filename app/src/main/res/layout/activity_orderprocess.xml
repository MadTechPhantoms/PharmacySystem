<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F6F6F6"
    tools:context=".orderprocess">

    <ImageView
        android:id="@+id/logo"
        android:layout_width="90dp"
        android:layout_height="31dp"
        android:layout_marginStart="250dp"
        android:layout_marginLeft="250dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="24dp"
        android:contentDescription="@string/logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.873"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.041"
        app:srcCompat="@drawable/caringpharmacy" />

    <ImageView
        android:id="@+id/backicon"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:contentDescription="@string/backicon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/logo"
        app:layout_constraintHorizontal_bias="0.138"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.039"
        app:srcCompat="?attr/actionModeCloseDrawable" />

    <View
        android:id="@+id/view"
        android:layout_width="379dp"
        android:layout_height="539dp"
        android:background="#DADADA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.531"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/date"
        android:layout_width="118dp"
        android:layout_height="45dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:text="@string/date"
        android:textColor="#455A64"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:layout_constraintVertical_bias="0.023"
        tools:text="@string/date" />

    <Button
        android:id="@+id/odcancel"
        android:layout_width="154dp"
        android:layout_height="48dp"
        android:text="@string/odcancel"
        android:textColor="#000000"
        app:backgroundTint="#5C5959"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/odprocess"
        app:layout_constraintTop_toBottomOf="@+id/odprocess2"
        tools:text="@string/odcancel" />

    <Button
        android:id="@+id/odprocess"
        android:layout_width="154dp"
        android:layout_height="48dp"
        android:layout_marginStart="36dp"
        android:layout_marginLeft="36dp"
        android:text="@string/odprocess"
        android:textColor="#000000"
        app:backgroundTint="#5C5959"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/odprocess2"
        app:layout_constraintVertical_bias="0.507"
        tools:text="@string/odprocess" />

    <TextView
        android:id="@+id/odprocess1"
        android:layout_width="240dp"
        android:layout_height="79dp"
        android:layout_marginBottom="284dp"
        android:fontFamily="sans-serif-black"
        android:text="@string/odprocess1"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/odprocess"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.561"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:layout_constraintVertical_bias="1.0"
        tools:text="@string/odprocess1" />

    <TextView
        android:id="@+id/odprocess2"
        android:layout_width="342dp"
        android:layout_height="247dp"
        android:fontFamily="sans-serif-black"
        android:text="@string/odprocess2"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:layout_constraintVertical_bias="0.619"
        tools:text="@string/odprocess2" />

    <TextView
        android:id="@+id/time"
        android:layout_width="118dp"
        android:layout_height="45dp"
        android:text="@string/time"
        android:textColor="#455A64"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/date"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:layout_constraintVertical_bias="0.023"
        tools:text="@string/time" />

</androidx.constraintlayout.widget.ConstraintLayout>